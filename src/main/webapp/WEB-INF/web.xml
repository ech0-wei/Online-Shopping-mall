<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <filter>
        <filter-name>CacheFilter</filter-name>
        <filter-class>CacheFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CacheFilter</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>


    <servlet>
      <servlet-name>HelloServlet</servlet-name>
      <servlet-class>HelloServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/HelloServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>GoodByeServlet</servlet-name>
        <servlet-class>GoodByeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GoodByeServlet</servlet-name>
        <url-pattern>/GoodByeServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>UserCheck</servlet-name>
        <servlet-class>UserCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserCheck</servlet-name>
        <url-pattern>/UserCheck</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SecurityUserPageCheck</servlet-name>
        <servlet-class>SecurityUserPageCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SecurityUserPageCheck</servlet-name>
        <url-pattern>/SecurityUserPageCheck</url-pattern>
    </servlet-mapping>

	<servlet>
		<servlet-name>Location</servlet-name>
		<servlet-class>LocationCheck</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Location</servlet-name>
		<url-pattern>/Location</url-pattern>
	</servlet-mapping>



</web-app>